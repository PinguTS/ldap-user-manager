objectClass: top
cn: org_admin
description: Organization administrator for Example Company
member: uid=admin@examplecompany.com,ou=users,o=Example Company,ou=organizations,dc=example,dc=com

# Example user accounts
dn: uid=admin@examplecompany.com,ou=users,o=Example Company,ou=organizations,dc=example,dc=com
objectClass: inetOrgPerson
objectClass: top
