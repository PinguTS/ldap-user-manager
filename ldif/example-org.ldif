# Example organization
dn: o=Example Company,ou=organizations,dc=example,dc=com
objectClass: top
objectClass: organization
objectClass: labeledURIObject
objectClass: extensibleObject
o: Example Company
postalAddress: 123 Business Street$10001$New York$NY$USA
telephoneNumber: +1-555-0123
