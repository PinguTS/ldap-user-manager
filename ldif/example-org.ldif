# Example organization
dn: o=Example Company,ou=organizations,dc=example,dc=com
objectClass: organization
objectClass: top
o: Example Company
postalAddress: 123 Business Street$New York$NY$10001$USA
telephoneNumber: +1-555-0123
labeledURI: https://examplecompany.com
mail: info@examplecompany.com

# Users organizational unit for this company
dn: ou=users,o=Example Company,ou=organizations,dc=example,dc=com
objectClass: organizationalUnit
ou: users

# Roles organizational unit for this company
dn: ou=roles,o=Example Company,ou=organizations,dc=example,dc=com
objectClass: organizationalUnit
ou: roles

# Organization admin role
dn: cn=org_admin,ou=roles,o=Example Company,ou=organizations,dc=example,dc=com
